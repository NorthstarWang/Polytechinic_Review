<div class="fix-header fix-sidebar card-no-border">
    <script>
        window.onload = function () {
            var data = [];
            {{#each polytechnics}}
            data.push({ y: {{ y }}, label: "{{label}}"});
            {{/each}}
                data.sort(function (first, second) {
                    return first.y - second.y;
                });
                var chart = new CanvasJS.Chart("chartContainer", {
                    animationEnabled: true,
                    title: {
                        text: "Schools by average user score"
                    },
                    axisY2: {
                        interlacedColor: "rgba(1,77,101,.2)",
                        gridColor: "rgba(1,77,101,.1)",
                        title: "Average User Score",
                        maximum: 5
                    },
                    data: [{
                        type: "bar",
                        name: "polytechnics",
                        axisYType: "secondary",
                        color: "#014D65",
                        dataPoints: data
                    }]
                });
                chart.render();
            }
            function newChart() {
                var chart = new CanvasJS.Chart("chartContainer", {
                    animationEnabled: true,
                    title: {
                        text: "Polytechnics by average user score"
                    },
                    axisY2: {
                        interlacedColor: "rgba(1,77,101,.2)",
                        gridColor: "rgba(1,77,101,.1)",
                        title: "Average User Score",
                        maximum: 5
                    },
                    data: [{
                        type: "bar",
                        name: "polytechnics",
                        axisYType: "secondary",
                        color: "#014D65",
                        dataPoints: [{ y: 1, label: "test" }]
                    }]
                });
                chart.render();
            }
    </script>
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" /> </svg>
    </div>
    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <div id="main-wrapper">
        <!-- ============================================================== -->

        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- End Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->
        <div class="page-wrapper" style="margin-left: 0;">
            <!-- ============================================================== -->
            <div class="w-100 p-5 text-center"
