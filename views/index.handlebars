                            </div>
                            <div>
                                <hr class="m-t-0 m-b-0">
                                <a href="/event2" class="btn btn-primary btn-lg" role="button" aria-disabled="true">View</a>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- ============================================================== -->
                <!-- End PAge Content -->
                <!-- ============================================================== -->
                <!-- ============================================================== -->
                <h2>Here are some Reviews:</h2>
                <div class="row">
                    {{#each reviewObject}}
                    <div class="col-lg-4 col-md-7 mt-2">
                        <div class="card">
                            {{#each ../UserObject}}
                            {{#compare id ../userId}}
                            <img class="card-img-top" src="../assets/images/background/profile-bg.jpg"
                                alt="Card image cap">
                            <div class="card-body little-profile text-center">
                                <div class="pro-img"><img src="{{profilepic}}" alt="user" /></div>
                                <h3 class="m-b-0">{{../title}}</h3>
                                    <h5 style="display:inline-block;">by {{username}}</h5>
                                    <h5 style="">reviewing {{../reviewschoolname}}</h5>
                                    {{/compare}}
                                    {{/each}}
                                <div>
                                    <br>
                                <h5 style="text-decoration: underline;">Feedback to management:</h5>
                                <p> {{feedbackdesc}}</p>
                                <p> Final Rating: {{rating}}</p>
                                </div>
                            </div>
