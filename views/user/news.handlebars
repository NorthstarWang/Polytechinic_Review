<body class="fix-header fix-sidebar card-no-border">
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <div>
       
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- End Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->
        <div class="page-wrapper" style="margin-left: 4;">
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">
                <!-- Bread crumb and right sidebar toggle -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="tab-content">
                                <div class="tab-pane active" id="home" role="tabpanel">
                                    <div class="card-body">
                                        <div class="profiletimeline">
											{{#each news}}
                                            <div class="sl-item">
                                                
                                                <div class="sl-right text-white">
                                                    <div><a href="#" class="link">Don</a> <span class="sl-date">{{update_date_str}}</span>
                                                        <p>{{title}} check this out <a href="/news_detail?id={{id}}">{{tit}}</a></p>
                                                        <div class="row">
                                                            <div class="col-lg-3 col-md-6 m-b-20"><a href="/news_detail?id={{id}}" ><img src="{{url}}" alt="{{title}}" class="img-responsive radius" /></a></div>
                                                        </div>
                                                        <div class="like-comm"> <a href="javascript:void(0)" class="link m-r-10">2 comment</a> <a href="javascript:void(0)" class="link m-r-10"><i class="fa fa-heart text-danger"></i> 5 Thumb Up</a> </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <hr>
											{{/each}}
                                            
                                        </div>
                                        {{#compare user.status "Admin"}}
                                        <button type="submit" class="btn btn-success m-t-20" onclick="location.href='/news_compose'"><i class="fa fa-envelope-o" ></i> Create News</button>
                                        {{/compare}}
                                    </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Column -->
                    
                </div>
                <!-- ============================================================== -->
                <!-- End PAge Content -->
                <!-- ============================================================== -->
               
            </div>
            <!-- ============================================================== -->
            <!-- End Container fluid  -->
            <!-- ============================================================== -->
        </div>
        <!-- ============================================================== -->
            <!-- End Page wrapper  -->
        <!-- ============================================================== -->
    </div>
